# Deployment configuration - EXAMPLE FILE
# Copy this file to .env and replace with your own values
# IMPORTANT: NEVER commit the actual .env file to version control!

# Ethereum wallet and API keys
DEPLOYER_PRIVATE_KEY=your_wallet_private_key_without_0x_prefix
INFURA_PROJECT_ID=your_infura_project_id
INFURA_API_KEY=your_infura_api_key
ETHERSCAN_API_KEY=your_etherscan_api_key

# RPC URLs
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your_api_key
# or
# SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_api_key

# Initial contract parameters
INITIAL_SUPPLY=1000000  # Initial token supply
MINT_FEE=10             # Fee to mint an NFT (in tokens)
TRANSACTION_FEE=2       # Fee for transactions (in tokens)